<div class="container">
  <%= form_for(@tool, url: admin_tools_path ) do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name  %>
    <br>

    <%= f.label :use %>
    <%= f.text_field :use %>
    <br>

    <%= f.label :user_id %>
    <%= f.text_field :user_id %>

    <br>
    <br><br>

    <%= f.submit "Submit", class: "waves-effect btn" %>
  <% end  %>
</div>
